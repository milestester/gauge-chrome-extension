{
  "manifest_version": 2,

  "name": "Time Waste Extension",
  "description": "This extension gives a breakdown of time wasted on certain websites.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],

  "background" : {
    "scripts": ["eventPage.js"],
    "persistent": false
  }
}